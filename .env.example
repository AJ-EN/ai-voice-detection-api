# Environment configuration template
# Copy this to .env and update values for your environment

# API Security - REQUIRED: Change this in production!
API_KEY=dev-key-change-in-production

# Performance
WORKERS=1
REQUEST_TIMEOUT=30

# Audio Processing
SAMPLE_RATE=16000
MAX_AUDIO_DURATION=60.0

# Model Settings
MODEL_PATH=./models/voice_classifier.pkl
DEVICE=cpu

# Rate Limiting
RATE_LIMIT_PER_MINUTE=60

# Logging
LOG_LEVEL=INFO
